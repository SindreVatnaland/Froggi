{
	"appId": "com.slpRankClient.app",
	"productName": "slpRank-client",
	"directories": {
		"output": "dist"
	},
	"artifactName": "slpRank-client-${version}-${os}.${ext}",
	"win": {
		"icon": "bin/static/icon.png",
		"target": [
			{
				"target": "nsis",
				"arch": ["x64"]
			}
		]
	},
	"linux": {
		"icon": "bin/static/icon.png",
		"target": ["default"]
	},
	"mac": {
		"icon": "bin/static/icon.png",
		"target": ["default"],
		"extraResources": ["res", "favicon.png"]
	},
	"executableName": "slp-rank-client",
	"files": [
		"frontend/build/**/*",
		"build/index.html",
		"build/**/*",
		"**/*",
		"electron/**/*",
		"electron/utils/**/*",
		{
			"from": "build",
			"to": ""
		}
	]
}
